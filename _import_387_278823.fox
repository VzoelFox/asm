fungsi str_substring_after(s, start_index)
  var len_sub = str_len(s)
  var res_str = ""
  jika (start_index < len_sub)
     var new_len = len_sub - start_index
     var temp = str_slice(s, start_index, new_len)
     res_str = temp
  tutup_jika
  asm_mulai
  mov rax, [var_res_str]
  tutup_asm
tutup_fungsi

